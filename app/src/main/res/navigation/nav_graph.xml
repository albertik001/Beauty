<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    tools:ignore="InvalidNavigation">
    <action
        android:id="@+id/action_global_to_authenticationFlowFragment"
        app:destination="@id/authenticationFlowFragment"
        app:popUpTo="@id/nav_graph" />
    <action
        android:id="@+id/action_global_to_mainFlowFragment"
        app:destination="@id/mainFlowFragment"
        app:popUpTo="@id/nav_graph" />

    <fragment
        android:id="@+id/authenticationFlowFragment"
        android:name="com.geektech.beauty.presentation.ui.fragments.authentication.AuthenticationFlowFragment"
        android:label="fragment_authentication_flow"
        tools:layout="@layout/fragment_authentication_flow">

    </fragment>
    <fragment
        android:id="@+id/mainFlowFragment"
        android:name="com.geektech.beauty.presentation.ui.fragments.main.MainFlowFragment"
        android:label="fragment_main_flow"
        tools:layout="@layout/fragment_main_flow" />
</navigation>